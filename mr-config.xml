            <hive-column-type>string</hive-column-type>
        </column-mapping>
        <column-mapping>
            <hive-column-name>fast_charge</hive-column-name>
            <hive-column-type>string</hive-column-type>
        </column-mapping>
        <column-mapping>
            <hive-column-name>network</hive-column-name>
            <hive-column-type>string</hive-column-type>
        </column-mapping>
        <column-mapping>
            <hive-column-name>code</hive-column-name>
            <hive-column-type>string</hive-column-type>
        </column-mapping>
    </mapping-info>
</config>

