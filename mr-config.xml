<?xml version="1.0" encoding="UTF-8" ?>
<config>
    <!-- Global Settings -->
    <input-path>hdfs://nameservice1/user/hive/warehouse/anti_fraud.db/expand_blacklist_output_r</input-path>
    <output-path>hdfs://nameservice1/tmp/hfile-output-test</output-path>
    <htable-name>fraud:blacklist_v2</htable-name>
    <field-delimiter>\u0001</field-delimiter>
    <collection-item-delimiter>,</collection-item-delimiter>
    <hbase.zookeeper.quorum>192.168.254.67,192.168.254.68,192.168.255.66</hbase.zookeeper.quorum>
    <hbase.zookeeper.property.clientPort>2181</hbase.zookeeper.property.clientPort>
    <hbase.zookeeper.property.maxClientCnxns>400</hbase.zookeeper.property.maxClientCnxns>
    <hbase.znode.parent>/hbase</hbase.znode.parent>

    <!-- Local Settings -->
    <!-- 1 -->
    <mapping-info>
        <partition>
            data_date=20161231/list=expand,
            data_date=20161231/list=init,
            data_date=20170131/list=expand,
            data_date=20170131/list=init,
            data_date=20170228/list=expand,
            data_date=20170228/list=init,
            data_date=20170331/list=expand,
            data_date=20170331/list=init,
            data_date=20170430/list=expand,
            data_date=20170430/list=init
        </partition>
        <column-mapping>
            <hive-column-name>imei</hive-column-name>
            <hive-column-type>string</hive-column-type>
            <rowkey>true</rowkey>
        </column-mapping>
        <column-mapping>
            <hive-column-name>cnt</hive-column-name>
            <hive-column-type>bigint</hive-column-type>
        </column-mapping>
        <column-mapping>
            <hive-column-name>prob</hive-column-name>
            <hive-column-type>double</hive-column-type>
            <hbase-column-family>A</hbase-column-family>
            <hbase-column-qualifier>RI_CIRCLE_PROB</hbase-column-qualifier>
        </column-mapping>
    </mapping-info>

</config>